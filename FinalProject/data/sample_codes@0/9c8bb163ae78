static void igmpv3_del_delrec(struct in_device *in_dev, struct ip_mc_list *im)				psf->sf_crcount = im->crcount;
		}
		in_dev_put(pmc->interface);
		kfree(pmc);
	}
	spin_unlock_bh(&im->lock);
}

static void mld_del_delrec(struct inet6_dev *idev, struct ifmcaddr6 *im)				psf->sf_crcount = im->mca_crcount;
		}
		in6_dev_put(pmc->idev);
		kfree(pmc);
	}
	spin_unlock_bh(&im->mca_lock);
}


